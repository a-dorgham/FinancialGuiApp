QMainWindow {
    background-color: #f0f0f0;
}

QDockWidget {
    font-size: 12px;
}

QPushButton {
    background-color: #4CAF50;
    color: white;
    border: none;
    padding: 5px 10px;
    border-radius: 3px;
}

QPushButton:hover {
    background-color: #45a049;
}

QLineEdit {
    border: 1px solid #ccc;
    padding: 3px;
    border-radius: 3px;
}

QTextEdit {
    background-color: #ffffff;
    border: 1px solid #ccc;
}